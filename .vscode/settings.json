{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/webapp": true,
        "**/node_modules": true,
        "**/.gitignore": true,
        "**/package*": true,
        "**/public": true,
        "**/.vscode": true,
        "**/*.code-workspace": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true
    }
}